# .gtkrc-2.0 should symlink to this file

gtk-theme-name = "Marion-dark"
gtk-icon-theme-name = "Marion-dark"
gtk-cursor-theme-name="Vanilla-DMZ"

gtk-icon-sizes = "panel-menu=16,16: panel=16,16: gtk-menu=16,16: gtk-small-toolbar=16,16: gtk-large-toolbar=16,16: gtk-dialog=32,32: gtk-button=16,16"
gtk-toolbar-style = GTK_TOOLBAR_TEXT
gtk-menu-images = 0

gtk-recent-files-max-age = 0
gtk-recent-files-limit = 0

show-other-locations = 0
