{
  "extends": [
    "eslint:recommended"
  ],

  "parserOptions": {
    "ecmaVersion": 9
  },

  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },

  "rules": {
    "comma-dangle": 1,
    "no-console": 0,
    "no-tabs": 1,
    "no-trailing-spaces": 1,
    "quotes": [1, "single"],
    "semi": 1
  }
}
