"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"$HOME/bin/volume toggle"
  XF86AudioMute

"$HOME/bin/volume down"
  XF86AudioLowerVolume

"$HOME/bin/volume up"
  XF86AudioRaiseVolume

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"rhythmbox-client --no-start --previous"
  XF86AudioPrev

"rhythmbox-client --no-start --play-pause"
  XF86AudioPlay

"rhythmbox-client --no-start --stop"
  Control + XF86AudioPlay

"rhythmbox-client --no-start --next"
  XF86AudioNext

"playerctl-mute-toggle"
  Alt + F10

"playerctl -p $PLAYER_NAME volume 0.05-"
  Alt + XF86AudioLowerVolume

"playerctl -p $PLAYER_NAME volume 0.05-"
  Alt + F11

"playerctl -p $PLAYER_NAME volume 0.05+"
  Alt + XF86AudioRaiseVolume

"playerctl -p $PLAYER_NAME volume 0.05+"
  Alt + F12

"playerctl -p $PLAYER_NAME previous"
  Alt + F7

"playerctl -p $PLAYER_NAME play-pause"
  Alt + F8

"playerctl -p $PLAYER_NAME next"
  Alt + F9

"playerctl -p $PLAYER_NAME position 3-"
  Alt + minus

"playerctl -p $PLAYER_NAME position 3+"
  Alt + equal # plus

"one catfish"
  XF86Search

"toggle rofi-run rofi"
  Hyper_L # caps:hyper

"gnome-system-monitor --show-resources-tab"
  Control + Shift + Escape

"roxterm --separate"
  Alt + Escape

"lock"
  Mod4 + l

"lockdown"
  Alt + Mod4 + l

"thunar $HOME"
  Alt + grave

"backlight --down"
  XF86MonBrightnessDown

"backlight --up"
  XF86MonBrightnessUp

"display-mode night"
  Control + XF86MonBrightnessDown

"display-mode day"
  Control + XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"one xfce4-settings-manager"
  XF86Tools

"xkill"
  Control + Alt + f
