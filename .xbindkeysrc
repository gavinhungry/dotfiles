"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"$HOME/bin/volume toggle"
  XF86AudioMute

"$HOME/bin/volume down"
  XF86AudioLowerVolume

"$HOME/bin/volume up"
  XF86AudioRaiseVolume

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"one catfish"
  XF86Search

"disp-day"
  Control + XF86MonBrightnessUp

"disp-night"
  Control + XF86MonBrightnessDown

"one gmrun"
  Alt_R

"gnome-system-monitor"
  Control + Shift + Escape

"roxterm --separate"
  Alt + Escape

"lock"
  Mod4 + l

"thunar $HOME"
  Alt + grave

"xbacklight -dec 5"
  XF86MonBrightnessDown

"xbacklight -inc 5"
  XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"one xfce4-settings-manager"
  XF86Tools

"xkill"
  Control + Alt + f
