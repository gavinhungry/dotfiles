"$HOME/bin/viewport prev"
  Mod4 + Escape

"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --borderless-window"
  Control + Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"$HOME/bin/volume toggle"
  XF86AudioMute

"$HOME/bin/volume down"
  XF86AudioLowerVolume

"$HOME/bin/volume up"
  XF86AudioRaiseVolume

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"playerctl -i $CASTCTL_PLAYER play-pause"
  XF86AudioPlay

"playerctl -i $CASTCTL_PLAYER stop"
  Control + XF86AudioPlay

"playerctl -i $CASTCTL_PLAYER previous"
  XF86AudioPrev

"playerctl -i $CASTCTL_PLAYER next"
  XF86AudioNext

"playerctl-castctl previous"
  Alt + F7

"playerctl-castctl play-pause"
  Alt + F8

"playerctl-castctl next"
  Alt + F9

"castctl-mute-toggle"
  Alt + F10

"playerctl-castctl volume 0.05-"
  Alt + F11

"playerctl-castctl volume 0.05+"
  Alt + F12

"castctl-mute-toggle"
  Alt + XF86AudioMute

"playerctl-castctl volume 0.05-"
  Alt + XF86AudioLowerVolume

"playerctl-castctl volume 0.05+"
  Alt + XF86AudioRaiseVolume

"playerctl-castctl position 3-"
  Alt + minus

"playerctl-castctl position 3+"
  Alt + equal # plus

"one catfish"
  XF86Search

"toggle rofi-run rofi"
  XF86Open

"gnome-system-monitor --show-resources-tab"
  Control + Shift + Escape

"roxterm --separate"
  Alt + Escape

"lock"
  Mod4 + l

"lockdown"
  Alt + Mod4 + l

"thunar $HOME"
  Alt + grave

"next-window"
  Alt + Tab

"backlight --down"
  XF86MonBrightnessDown

"backlight --up"
  XF86MonBrightnessUp

"display-mode night"
  Control + XF86MonBrightnessDown

"display-mode day"
  Control + XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"one xfce4-settings-manager"
  XF86Tools

"xkill"
  Control + Alt + f
