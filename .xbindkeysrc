"$HOME/bin/viewport prev"
  Mod4 + Escape

"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --borderless-window"
  Control + Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"$HOME/bin/volume toggle"
  XF86AudioMute

"$HOME/bin/volume down"
  XF86AudioLowerVolume

"$HOME/bin/volume up"
  XF86AudioRaiseVolume

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"playerctl previous"
  XF86AudioPrev

"playerctl next"
  XF86AudioNext

"playerctl play-pause"
  XF86AudioPlay

"playerctl stop"
  Control + XF86AudioPlay

"castd prev"
  Alt + F9

"castd next"
  Alt + F10

"castd playpause"
  Alt + c:191 # F13/XF86Tools

"castd stop"
  Control + Alt + c:191 # F13/XF86Tools

"castd vol-down"
  Alt + F11

"castd vol-up"
  Alt + F12

"castd vol-down"
  Alt + XF86AudioLowerVolume

"castd vol-up"
  Alt + XF86AudioRaiseVolume

"castd mutetoggle"
  Alt + XF86AudioMute

"one catfish"
  XF86Search

"toggle rofi-run rofi"
  XF86Open

"gnome-system-monitor --show-resources-tab"
  Control + Shift + Escape

"roxterm --separate"
  Alt + Escape

"lock"
  Mod4 + l

"lockdown"
  Alt + Mod4 + l

"thunar $HOME"
  Alt + grave

"next-window"
  Alt + Tab

"backlight --down"
  XF86MonBrightnessDown

"backlight --up"
  XF86MonBrightnessUp

"display-mode night"
  Control + XF86MonBrightnessDown

"display-mode day"
  Control + XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"xkill"
  Control + Alt + f
