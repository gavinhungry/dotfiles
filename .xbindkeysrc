"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"amixer -q set ${MASTER_VOLUME_MIXER:-Master} playback 1dB+"
  XF86AudioRaiseVolume

"amixer -q set ${MASTER_VOLUME_MIXER:-Master} playback 1dB-"
  XF86AudioLowerVolume

"amixer -q set ${MASTER_VOLUME_MIXER:-Master} playback toggle"
  XF86AudioMute

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"one catfish"
  XF86Search

"disp-day"
  Control + XF86MonBrightnessUp

"disp-night"
  Control + XF86MonBrightnessDown

"one gmrun"
  Alt_R

"gnome-system-monitor"
  Control + Shift + Escape

"/etc/scripts/mute.sh"
  Control + XF86AudioMute

"roxterm --separate"
  Alt + Escape

"slimlock --alt"
  Mod4 + Alt + l

"slimlock"
  Mod4 + l

"thunar $HOME"
  Alt + grave

"xbacklight -dec 2"
  XF86MonBrightnessDown

"xbacklight -inc 2"
  XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"one xfce4-settings-manager"
  XF86Tools

"xkill"
  Control + Alt + f
