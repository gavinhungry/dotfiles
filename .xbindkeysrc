"$HOME/bin/shot --fullscreen"
  Print

"$HOME/bin/shot --window"
  Mod4 + Print

"$HOME/bin/shot --region"
  Alt + Print

"playerctl previous"
  XF86AudioPrev

"playerctl play-pause"
  XF86AudioPlay

"playerctl next"
  XF86AudioNext

"playerctl previous"
  Alt + F7

"playerctl play-pause"
  Alt + F8

"playerctl next"
  Alt + F9

"playerctl volume 0.0"
  Alt + F10

"playerctl volume 0.05-"
  Alt + F11

"playerctl volume 0.05+"
  Alt + F12

"$HOME/bin/volume toggle"
  XF86AudioMute

"$HOME/bin/volume down"
  XF86AudioLowerVolume

"$HOME/bin/volume up"
  XF86AudioRaiseVolume

"amixer -q set Capture capture toggle"
  XF86AudioMicMute

"one catfish"
  XF86Search

"one gmrun"
  Alt_R

"gnome-system-monitor --show-resources-tab"
  Control + Shift + Escape

"roxterm --separate"
  Alt + Escape

"lock"
  Mod4 + l

"lockdown"
  Alt + Mod4 + l

"thunar $HOME"
  Alt + grave

"xbacklight -dec 2"
  XF86MonBrightnessDown

"xbacklight -inc 2"
  XF86MonBrightnessUp

"display-night"
  Control + XF86MonBrightnessDown

"display-day"
  Control + XF86MonBrightnessUp

"one xfce4-appfinder --disable-server"
  XF86Explorer

"one xfce4-display-settings --minimal"
  XF86Display

"one xfce4-settings-manager"
  XF86Tools

"xkill"
  Control + Alt + f
